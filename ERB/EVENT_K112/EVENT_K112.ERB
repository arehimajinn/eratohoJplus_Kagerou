;----------------------------------------------------------------
@PRESENT_K112
#FUNCTIONS
#DIMS PRESENT
#DIMS LIST, 150

IF TALENT:相愛 && FIRSTTIMES("贈り物_指輪")
	;指輪
	RETURNF "指輪"
ELSEIF TALENT:烙印 && FIRSTTIMES("贈り物_首輪")
	;首輪、押した場合持ってないのよね
	RETURNF "首輪"
ELSEIF TALENT:烙印 && FIRSTTIMES("贈り物_手錠")
	;束縛する系のものがいいのかな的な発想
	RETURNF "手錠"
ELSEIF TALENT:親愛 && FIRSTTIMES("贈り物_ネックレス")
	RETURNF "ネックレス"
ELSEIF (TALENT:恋慕 || TALENT:恋人) && FIRSTTIMES("贈り物_ルピナス")
	;花言葉がなんとか
	RETURNF "ルピナスの花"
ENDIF

;何も無いなら他のものを贈る
