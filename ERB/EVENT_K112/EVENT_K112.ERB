IF GETCHARA(112) != -1
	;初期値セット
	SIF !CFLAG:(GETCHARA(112)):400
		CFLAG:(GETCHARA(112)):400 = 70
	;一ヶ月1cm伸びる、書きづらくてムカつく
	{
		IF
		((GETBIT(CFLAG:(GETCHARA(112)):340, 40) && CFLAG:(GETCHARA(112)):400 < 20)
		||
		(GETBIT(CFLAG:(GETCHARA(112)):340, 41) && CFLAG:(GETCHARA(112)):400 < 35)
		||
		(GETBIT(CFLAG:(GETCHARA(112)):340, 42) && CFLAG:(GETCHARA(112)):400 < 70))
		&&
		DAY:1 == 1 && !TIME
		
	}
		CFLAG:(GETCHARA(112)):400++
	ENDIF
ENDIF

;一年でリセットされるフラグの処理
;この口上では700台とリセットされるフラグとしている。簡単に流用できるのでぜひ。
IF DAY:1 == 1 && DAY:2 == 0 && !TIME
	FOR LOCAL, 800, 900
		CFLAG:LOCAL = 0
	NEXT
ENDIF

;上の一月版
IF DAY:1 == 1 && !TIME
	FOR LOCAL, 900, 1000
		CFLAG:LOCAL = 0
	NEXT
ENDIF

