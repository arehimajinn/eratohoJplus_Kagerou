;----------------------------------------------------------------
;数値を桁数ごとに分割して、数値を返す自分用関数
;ARG = フラグ, ARGS = イベント名
;----------------------------------------------------------------
@FLAG_SPLIT_K112(ARG, ARGS)
#FUNCTION

SELECTCASE ARGS
	CASE "ラジオ体操"
		;123456789 % 10 = 9
		RETURNF ARG % 10
	CASE "結婚記念日"
		;123456789 % 1000 = 789 / 10 = 78
		RETURNF (ARG % 1000) / 10
	CASE "会った回数"
		;123456789 % 100000 = 56789 / 1000 = 56
		;123450XXX % 100000 = 50XXX / 1000 = 50
		RETURNF (ARG % 100000) / 1000
ENDSELECT
